\section{formatowanie tekstow} 
\begin{itemize}
\huge \item  Czcionki \newline
\textit{abcabci} \\
\textup{abcabci} \\
\large \textbf{abcabc} \\
\uline{abcabc}\\
\Huge duze  \newline \\
\tiny mala \newline \\



\huge \item  Rozne kolory \newline \large

\textcolor{red}{Tecza}
\textcolor{yellow}{Tecza}
\textcolor{black}{Tecza}
\textcolor{green}{Tecza}
\textcolor{magenta}{Tecza}
\end{itemize}


